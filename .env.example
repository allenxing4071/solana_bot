# Solana网络配置
SOLANA_NETWORK=mainnet-beta
SOLANA_RPC_URL=https://api.mainnet-beta.solana.com
SOLANA_WS_URL=wss://mainnet.helius-rpc.com/?api-key=YOUR_HELIUS_API_KEY
USE_BACKUP_RPC=true
BACKUP_RPC_ENDPOINTS=https://api.mainnet-beta.solana.com,https://solana-api.projectserum.com

# 环境设置
NODE_ENV=production

# 钱包配置
WALLET_PRIVATE_KEY=YOUR_WALLET_PRIVATE_KEY
MAX_TRANSACTION_AMOUNT=0.1

# DEX程序ID
RAYDIUM_PROGRAM_ID=675kPX9MHTjS2zt1qfr1NYHuzeLXfQM9H24wFSUt1Mp8
ORCA_PROGRAM_ID=9W959DqEETiGZocYWCQPaJ6sBmUzgfxXfqGeTEdp3aQP

# 交易配置
TX_RETRY_COUNT=3
TX_CONFIRM_TIMEOUT=60000
TX_PRIORITY_FEE=0.000005

# 策略配置
BUY_AMOUNT_SOL=0.05
TAKE_PROFIT_PERCENTAGE=20
STOP_LOSS_PERCENTAGE=10
MAX_BUY_SLIPPAGE=5
MAX_SELL_SLIPPAGE=5

# 监控配置
HEALTH_CHECK_INTERVAL=30000
PRICE_CHECK_INTERVAL=5000

# 日志配置
LOG_LEVEL=info
LOG_TO_CONSOLE=true
LOG_TO_FILE=false

# 运行模式配置
LISTEN_ONLY=false

# API服务器配置
ENABLE_API_SERVER=true
API_PORT=8080

# SSH配置
SSH_HOST=YOUR_SSH_HOST
SSH_USER=YOUR_SSH_USER
SSH_PORT=22
SSH_KEY_PATH=YOUR_SSH_KEY_PATH
SSH_TIMEOUT=10000
SSH_LOG_LEVEL=info
SSH_LOG_FILE=ssh.log

# API设置
USE_MOCK_DATA=false

# Claude API代理服务器配置
CLAUDE_PROXY_PORT=3100

# 高级模型代理配置
OPENAI_API_KEY=YOUR_OPENAI_API_KEY
CURSOR_API_KEY=YOUR_CURSOR_API_KEY
OLLAMA_API_KEY=YOUR_OLLAMA_API_KEY
DEEPSEEK_API_KEY=YOUR_DEEPSEEK_API_KEY
GEMINI_API_KEY=YOUR_GEMINI_API_KEY

# 模型代理管理界面配置
ADMIN_DASHBOARD_PORT=3200
ADMIN_AUTH_ENABLED=false
ADMIN_USERNAME=admin
ADMIN_PASSWORD=YOUR_ADMIN_PASSWORD

# API配置
HELIUS_API_KEY=your_helius_api_key_here
JUPITER_API_URL=https://quote-api.jup.ag/v4

# 交易配置
TX_RETRY_COUNT=3
TX_CONFIRM_TIMEOUT=60000  # 毫秒
TX_PRIORITY_FEE=0.000005  # SOL

# 策略配置
BUY_AMOUNT_SOL=0.1
TAKE_PROFIT_PERCENTAGE=20  # 20%
STOP_LOSS_PERCENTAGE=10    # 10%
MAX_BUY_SLIPPAGE=5         # 5%
MAX_SELL_SLIPPAGE=5        # 5%

# 监控配置
HEALTH_CHECK_INTERVAL=30000  # 毫秒
PRICE_CHECK_INTERVAL=5000    # 毫秒

# 日志配置
LOG_LEVEL=info  # debug, info, warn, error
LOG_TO_FILE=true
LOG_FILE_PATH=./logs/bot.log

# Jito MEV配置（可选）
USE_JITO_BUNDLE=false
JITO_AUTH_KEYPAIR=your_jito_keypair_path_here

# 通知配置（可选）
ENABLE_TELEGRAM_NOTIFICATIONS=false
TELEGRAM_BOT_TOKEN=your_telegram_bot_token
TELEGRAM_CHAT_ID=your_telegram_chat_id

# 白名单/黑名单配置路径
TOKEN_WHITELIST_PATH=./config/whitelist.json
TOKEN_BLACKLIST_PATH=./config/blacklist.json 